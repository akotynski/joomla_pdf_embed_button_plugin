<?xml version="1.0" encoding="utf-8"?><extension version="3.1" type="plugin" group="editors-xtd" method="upgrade">    <name>Button - PDF template</name>    <author>akotynski</author>    <creationDate>June 2015</creationDate>    <copyright>Copyright (C) 2005 - 2015 Open Source Matters. All rights reserved.</copyright>    <license>GNU General Public License version 2 or later;</license>    <authorEmail>aleksanderkotbury@gmail.com</authorEmail>    <authorUrl></authorUrl>    <version>1.0</version>    <description>Pdf template for PDF Embed</description>    <files>        <filename plugin="pdfplugin">pdfplugin.php</filename>        <filename>pdf.php</filename>        <filename>pdfview.php</filename>        <filename>index.html</filename>    </files>    <config>        <fields name="params">            <fieldset name="basic">                <field name="width" type="text"                       default="570"                       description="pdf default width"                       label="pdf width"                       size="5"                        />                <field name="height" type="text"                       default="800"                       description="pdf default height"                       label="pdf height"                       size="5"                        />            </fieldset>        </fields>    </config></extension>